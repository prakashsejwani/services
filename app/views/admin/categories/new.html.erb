<% title 'New Category' %>
<div>
	<h3>New category</h3>
	<div class="clear"></div>
	<% form_for(:category, :url => admin_categories_path, :html => {:multipart => true}) do |f| %>
		<%= render :partial => 'category', :locals=>{:f=>f} %>
	<% end %>
	<br/>
	<span class="blue_link"><%= link_to 'Back', admin_categories_path %></span>
</div>
