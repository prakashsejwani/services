<% title @category.name %>
<div>
	<h3><%=h @category.name %></h3>
	<table>
		<tr><td>Alphabet:</td><td><%=h @category.alphabet %></td></tr>
		<tr><td>Business:</td><td><%=h Business.find(@category.business_id).name if @category.business_id %></td></tr>
	</table>
	<br/>
	<span class="blue_link"><%= link_to 'Edit', edit_admin_category_path(@category) %></span>
	<span class="blue_link"><%= link_to 'Back', admin_categories_path %></span>
</div>

