<% title 'Edit Locality' %>
<div>
	<h3>Edit locality</h3>
	<div class="clear"></div>
	<% form_for(:locality, :url => {:action=>:update}, :html=>{:method=>:put}) do |f| %>
	  <%=render :partial => 'locality', :locals=>{:f=>f}%>	
	<% end %>
	<br/>
	<span class="blue_link"><%= link_to 'Show', admin_locality_path(@locality) %></span>
	<span class="blue_link"><%= link_to 'Back', admin_localities_path %></span>
</div>
	
