		  <div id="left">
			<h3><%= @company.name if @company %></h3>
				<div><%= yield %></div>
			</div>
		  </div><!-- left end -->		  
		</div><!-- main end -->
		<div style=" width:760px; margin: 0 auto;">E-mail us your suggestions and feedback at services@econify.com</div>
